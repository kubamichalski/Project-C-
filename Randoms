#include <iostream>
#include <ctime>
#include <cstdlib>

//Wykorzystując zaprezentowane powyżej funkcje zadeklaruj tablicę 10 elementową i wypełnij ją losowymi wartościami z zakresu od 10 do 99. Następnie wypisz jej elementy. Instrukcję wypisania można zapisać w pętli wypełniającej tablicę.

using namespace std;

int main()
{
	srand(time(NULL));
  int tab[10] = {};
  
  for (int i=0; i<10 ; i++ )
  {
  	tab[i] = (rand() % 89) + 10;
    cout << tab[i] << endl;
  }
  
  return 0;
}
